<%- include('../partials/header') %>

<h1>Listado de productos</h1>
<ul>
  <% products.forEach(product => { %>
    <li>
      <a href="/products/detail/<%= product.id %>">
        <%= product.name %> - $<%= product.price %>
      </a>
    </li>
  <% }) %>
</ul>

